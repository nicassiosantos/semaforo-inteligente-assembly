013B EB                204             MOV A, R3 ; Verifica se está no verde mais
013C 5401              205             ANL A, #00000001b   ; Verifica bit 0 do R3
013E 6004              206             JZ VERDE_NORMAL     ; Se não estiver setado, tempo normal
0140 7D0F              207             MOV R5, #00001111b  ; Verde estendido (15 segundos)
0142 8002              208             SJMP INICIA_CONTAGEM
                       209
                       210     VERDE_NORMAL:
0144 7D0A              211             MOV R5, #00001010b  ; Verde normal (10 segundos)
                       212     INICIA_CONTAGEM:
0146 1132              213             CALL AGUARDA_TEMPO  ; Aguarda tempo configurado
0148 EA                214             MOV A, R2
0149 B40502            215             CJNE A, #00000101b, PROXIMO_ESTADO ; Verifica se o contador está em 5 veículos
014C 7B01              216             MOV R3, #00000001b      ; Ativa flag de verde mais para próxima vez
                       217     PROXIMO_ESTADO:
014E 7902              218             MOV R1, #2  ; Muda para estado amarelo
0150 80D0              219             JMP MAIN_LOOP
                       220
                       221     AMARELO:
0152 75A0FD            222             MOV P2 , #11111101b ; Ligar o led amarelo
0155 7D03              223             MOV R5, #00000011b    ; Configura 3 segundos
0157 1132              224             CALL AGUARDA_TEMPO    ; Aguarda o tempo
                       225
0159 7903              226             MOV R1, #00000011b    ; Próximo estado: vermelho (3)
015B 80C5              227             JMP MAIN_LOOP
                       228
                       229     VERMELHO:
015D 75A0FB            230             MOV P2, #11111011b    ; Liga LED vermelho
0160 7D07              231             MOV R5, #00000111b    ; Configura 7 segundos
0162 1132              232             CALL AGUARDA_TEMPO    ; Aguarda o tempo
                       233
0164 7B00              234             MOV R3, #00000000b      ; Reseta flag de verde mais
0166 7901              235             MOV R1, #00000001b    ; Próximo estado: verde (1)
0168 80B8              236             JMP MAIN_LOOP
                       237
                       238     INICIA_EMERGENCIA:
