                       137              MOV R0, #0         ; Zera R0
                       138              CJNE R2, #0, ESTADO_ANTERIOR ; Caso uma emergência tenha occorido, ele volta para o estado a
                       139              JMP AMARELO   ; Vai para o estado AMARELO
                       140
                       141     ESTADO_ANTERIOR:
                       142             MOV R2, #0 ;Zerando a flag de emergência
                       143             CJNE R1, #1, AMARELO_ANTERIOR; Caso não seja estado Verde Verifica se é amarelo
                       144             JMP VERDE ;Caso seja vermelho Volta para o estado Inicial de Vermelho
                       145     AMARELO_ANTERIOR:
                       146             CJNE R1, #2, VERDE ;Caso não seja estado Amarelo, é o estado Verde
                       147             JMP AMARELO ;Vai para o estado Amarelo
                       148
                       149     INICIO:
                       150         MOV IE, #10000011b ; Habilita interrupções (EA)
                       151                             ; interrupção do Timer 0 (ET0), interrupção externa (INT0)
                       152         MOV TH0, #11111110b     ; Configura TH0 com 0x3C (parte alta de 15536)
