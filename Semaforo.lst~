                       155         MOV TCON, #00010001b ; Liga o Timer 0 (TR0 = 1), habilita interrupção externa por borda no INT0
                       156
                       157     VERDE:
                       158             MOV R1, #1 ; Indicar que está no estado Verde
                       159             MOV R3, #0 ; Inicializa o contador de carros
                       160             MOV P2, #11111110b ;Ligar o led verde
                       161             JMP SETE        ;Jump para iniciar a contagem de sete segundos do Estado Verde
                       162             CJNE R3, #5, AMARELO ; Se o número de carros não tiver alcançado 5 continua pro amarelo
                       163
                       164             MOV R3, #0 ;Zera o contador de carros
                       165             JMP CINCO ; Aumenta duração do sinal verde
                       166             JMP AMARELO ; Vai para o estado amarelo
                       167
                       168     AMARELO:
                       169             MOV R1, #2 ;Indicar que está no estado Amarelo
                       170             MOV P2 , #11111101b ; Ligar o led amarelo
                       171             JMP TRES  ;Jump para Iniciar a contagem do Estado Amarelo
