                        21     TIMERSEC:
                        22             MOV TH0, #11111110b   ; Recarrega TH0 com 0x3C (parte alta de 15536)
                        23             MOV TL0, #00111111b   ; Recarrega TL0 com 0xB0 (parte baixa de 15536)
